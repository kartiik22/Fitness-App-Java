<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.endurofit">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.endurofit.SixDaySplit"></activity>
        <activity android:name="com.example.endurofit.FiveDaySplit" />
        <activity android:name="com.example.endurofit.Recommendation"/>
        <activity android:name="com.example.endurofit.FourDaySplit" />
        <activity android:name="com.example.endurofit.ThreeDaySplit" />
        <activity android:name="com.example.endurofit.Splits" />
        <activity android:name="com.example.endurofit.FoodCalories" />
        <activity android:name="com.example.endurofit.Timer" />
        <activity android:name="com.example.endurofit.HomePage" />
        <activity android:name="com.example.endurofit.UserProfile" />
        <activity android:name="com.example.endurofit.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>